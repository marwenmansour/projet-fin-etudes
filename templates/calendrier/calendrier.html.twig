{% extends 'base.html.twig' %}

{% block title %}Calendrier des saisons!{% endblock %}

{% block css %}
<link rel="stylesheet" href="{{ asset('css/calendrier.css') }}">
{% endblock %}

{% block body %}
  <div id="fond"></div>
        
        <!-- carousel -->
        <div class="container" id="carousel">
    
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">

    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>            
    </ol>

    <div class="carousel-inner" role="listbox">

        <!-- Printemps-->
        <div class="carousel-item" style="background-image: url('images/juin.jpg')">
          <div class="carousel-caption d-none d-md-block">
            <h3>Printemps</h3>
            <button type="button" class="btn btn-lg btn-primary" id="btn-prin">Voir les légumes & les fruits de la saison</button>
          </div>
        </div>

        <!-- Été-->
        <div class="carousel-item" style="background-image: url('images/juillet.jpg')">
          <div class="carousel-caption d-none d-md-block">
            <h3>Été</h3>
            <button type="button" class="btn btn-lg btn-primary" id="btn-ete">Voir les légumes & les fruits de la saison</button>
          </div>
        </div>      

         <!-- Automne-->
        <div class="carousel-item" style="background-image: url('images/novembre.jpg')">
          <div class="carousel-caption d-none d-md-block">
            <h3>Automne</h3>
            <button type="button" class="btn btn-lg btn-primary" id="btn-aut">Voir les légumes & les fruits de la saison</button>
          </div>
        </div>

        <!-- Hiver-->
        <div class="carousel-item active" style="background-image: url('images/decembre.jpg')">
          <div class="carousel-caption d-none d-md-block">
            <h3>Hiver</h3>
            <button type="button" class="btn btn-lg btn-primary" id="btn-hiv">Voir les légumes & les fruits de la saison</button>
          </div>
        </div>

    </div>

        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
  


</div>
      
<!-- carousel finish -->
<h1 id="titre">Les calendriers des légumes <br> & fruits de la saison</h1>
 
